{
    "name": "arnakoguzhan/laravel-scim-server",
    "description": "Laravel Package for creating a SCIM server",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arie Timmerman",
            "email": "arietimmerman@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "protonlabs/scim-schema": "^0.2",
        "tmilos/scim-filter-parser": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "ArieTimmerman\\Laravel\\SCIMServer\\": "src/",
            "ArieTimmerman\\Laravel\\SCIMServer\\Database\\Factories\\": "database/factories/"
        },
        "classmap": [
            "database/migrations"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ArieTimmerman\\Laravel\\SCIMServer\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArieTimmerman\\Laravel\\SCIMServer\\ServiceProvider"
            ],
            "aliases": {
                "SCIMServerHelper": "ArieTimmerman\\Laravel\\SCIMServer\\Helper"
            }
        }
    }
}
